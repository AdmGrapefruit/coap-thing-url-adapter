{
  "name": "coap-thing-url-adapter",
  "display_name": "CoAP Web Thing",
  "version": "0.1.0",
  "description": "CoAP web thing support",
  "author": "Mozilla IoT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ."
  },
  "keywords": [
    "mozilla",
    "iot",
    "adapter",
    "wot"
  ],
  "homepage": "https://github.com/mozilla-iot/thing-url-adapter",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla-iot/thing-url-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/mozilla-iot/thing-url-adapter/issues"
  },
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "coap-thing-url-adapter.js",
    "index.js"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "enabled": true,
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "config": {
      "urls": []
    },
    "schema": {
      "type": "object",
      "required": [
        "urls"
      ],
      "properties": {
        "urls": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "dependencies": {
    "coap": "github:AdmGrapefruit/node-coap#master",
    "dnssd": "^0.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0"
  }
}
